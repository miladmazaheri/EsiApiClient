<Window x:Class="IPAClient.Windows.wndReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPAClient.Windows"
        mc:Ignorable="d"
        Title="wndReport" Style="{StaticResource AppWindow}" Width="1280" Height="720" Loaded="WndReport_OnLoaded" >
    <Grid>
        <StackPanel>
            <DataGrid Name="grdReport" Height="500" IsReadOnly="True" FontFamily="{StaticResource Vazir}" FontSize="18"/>
            <StackPanel Height="100"></StackPanel>
            <StackPanel Height="105"></StackPanel>
        </StackPanel>
     </Grid>
</Window>
