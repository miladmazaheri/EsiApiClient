<Window x:Class="EsiApiClient.Windows.wndConfirmConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EsiApiClient.Windows"
        mc:Ignorable="d"
        Title="wndConfirmConfig" Height="768" Width="1368" WindowStyle="None">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="../Images/ConfirmConfig.png"/>
        </Grid.Background>
        <TextBox x:Name="txtRestaurant_Cod" HorizontalAlignment="Left" Margin="508,153,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <TextBox x:Name="txtDevice_Category" HorizontalAlignment="Left" Margin="508,213,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <TextBox x:Name="txtDevice_Type" HorizontalAlignment="Left" Margin="508,274,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <TextBox x:Name="txtDevice_Name" HorizontalAlignment="Left" Margin="508,337,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <TextBox x:Name="txtDevice_Cod" HorizontalAlignment="Left" Margin="508,398,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <TextBox x:Name="txtIP" HorizontalAlignment="Left" Margin="508,464,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <TextBox x:Name="txtNum_Queue" HorizontalAlignment="Left" Margin="508,530,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"
                 Width="212" Height="54" TextAlignment="Center" VerticalContentAlignment="Center"
                 Foreground="White" FontSize="28" BorderBrush="Transparent" Background="#00000000" BorderThickness="0"  >
        </TextBox>
        <Label x:Name="btnConfirm" Content="" HorizontalAlignment="Left" Margin="637,595,0,0" VerticalAlignment="Top" Width="223" Height="49" MouseDown="BtnConfirm_OnMouseDown"/>
        <Label x:Name="btnCancel" Content="" HorizontalAlignment="Left" Margin="507,594,0,0" VerticalAlignment="Top" Width="118" Height="49" MouseDown="BtnCancel_OnMouseDown"/>

    </Grid>
</Window>
